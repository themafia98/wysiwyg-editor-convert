{
  "name": "wysiwyg-editor-convert",
  "private": false,
  "version": "0.1.0",
  "description": "Convert react-draft-wysiwyg to html",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "repository": "themafia98/wysiwyg-editor-convert",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir esm",
    "build:umd": "wp",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i",
    "test-once": "npm run jest",
    "test": "npm run jest",
    "clean": "rimraf ./dist ./lib ./esm",
    "build-and-test": "npm run clean && npm run build && npm run test-once",
    "lint": "eslint src/ test/",
    "prepare": "npm run build-and-test",
    "prepublish": "npm run lint && npm run build-and-test",
    "prettier:write": "prettier --write ./{src,test}/*.js ./{src,test}/**/*.js"
  },
  "files": [
    "dist",
    "lib",
    "esm"
  ],
  "keywords": [
    "draft",
    "draftjs",
    "draft-convert",
    "convert"
  ],
  "author": "gaara33377@gmail.com",
  "license": "Apache-2.0",
  "peerDependencies": {
    "draft-js": ">=0.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "draft-convert": "^2.1.10",
    "immutable": "~4.0.0-rc.12",
    "invariant": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.3",
    "draft-js": "^0.11.7",
    "eslint": "7.21.0",
    "eslint-config-hubspot": "^7.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "7.22.0",
    "jasmine": "^3.6.4",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "3.0.2",
    "webpack": "^5.24.4",
    "webpack-nano": "^1.1.1"
  },
  "jest": {
    "testRegex": "/test/spec/.*\\.js$"
  }
}
